// Job Searching Portal Flowchart
digraph {
	A [label="Job Seeker"]
	B [label="Job Search"]
	C [label="View Job Listings"]
	D [label="Apply for Job"]
	E [label="View Application Status"]
	F [label=Employer]
	G [label="Post Job"]
	H [label="Manage Job Listings"]
	I [label="Review Applications"]
	J [label="Conduct Hackathon"]
	K [label=Admin]
	L [label=Login]
	M [label=Dashboard]
	N [label="Manage Users"]
	O [label="Manage Job Postings"]
	P [label="Review Reports"]
	A -> B
	B -> C
	C -> D
	D -> E
	A -> F
	F -> G
	F -> H
	F -> I
	F -> J
	A -> K
	K -> L
	L -> M
	M -> N
	M -> O
	M -> P
}
